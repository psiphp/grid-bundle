<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="psi_grid.cell_registry" class="Psi\Component\View\TypeRegistry" public="false" />

        <service id="psi_grid.cell_factory" class="Psi\Component\View\ViewFactory" public="false" >
            <argument type="service" id="psi_grid.cell_registry" />
