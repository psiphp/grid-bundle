{
    "name": "psiphp/grid-bundle",
    "description": "Locate and render Twig templates based on object FQCNs",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "^3.0",
        "twig/twig": "^1.0",
        "psiphp/object-render-bundle": "^1.0@dev",
        "psiphp/grid": "^1.0@dev"
    },
    "require-dev": {
        "symfony/console": "^3.0",
        "symfony/yaml": "^3.0",
        "symfony/process": "^3.0",
        "phpunit/phpunit": "@stable",
        "symfony/translation": "^3.1",
        "symfony/validator": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Psi\\Bundle\\Grid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Psi\\Bundle\\Grid\\Tests\\": "tests/",
            "Psi\\Bundle\\Grid\\Example\\": "example/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
